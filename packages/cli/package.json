{
  "name": "@schemasync/cli",
  "version": "0.1.0",
  "description": "Command line client for SchemaSync",
  "type": "module",
  "bin": {
    "schemasync": "dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
