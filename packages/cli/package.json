  },
  "dependencies": {
    "@schemasync/core": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.1.0",
    "vitest": "^1.6.0"
  }
}
